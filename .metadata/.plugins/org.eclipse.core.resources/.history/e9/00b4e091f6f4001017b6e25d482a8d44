package exam;

import java.util.Scanner;

public class Q2739 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner keyboard = new Scanner(System.in);
		
		int n = 0;
		int i = 1;
		
		n = keyboard.nextInt();
		
		while(i < 10 ) {
			
			System.out.println(n + "*" + --i + "=" + n * i );
			
			
		}

	}

}
