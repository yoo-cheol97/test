		//배열 + for + while
		
		int[] luckyNum = new int[7];
		
		int count = 0;
		boolean existsNumber = false;
		int randomNumber = 0;
		
		while(count < 7) {
			randomNumber = (int) (Math.random() * 45 + 1);
			existsNumber = false;
			
			for (int i = 0; i < count; i++) {
				if (luckyNum[i] == randomNumber) {
					existsNumber = true;
					break;
				}
			}
		
			if(!existsNumber) {
				luckyNum[count] = randomNumber;
				count++;
		}
		
		for(int i = 0; i < luckyNum.length; i++) {
			System.out.println(luckyNum[i] +"");
		}
		System.out.println("");
		
		
		
		
		
		
		
		
		/*
