public class Q2525 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int nowTimeH = 0;
		int nowTimeM = 0;
		int cookTime = 0;
		
		nowTimeH = 14;
		nowTimeM = 30;
		cookTime = 20;
		
		
		
		if (nowTimeM + cookTime < 60) {
			System.out.println(nowTimeH  );
		}

	}

