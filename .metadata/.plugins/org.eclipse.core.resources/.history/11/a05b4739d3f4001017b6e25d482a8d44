package exam;

public class Q2753 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int year = 0;
		
		year = 1999;
		
		
		if(year % 4 == 0) {
			if(year % 100 != 0) {
				System.out.println("1");
			}
			else if(year % 400 == 0) {
				System.out.println("1");
			}
			
		}
		else {
			System.out.println("0");
		}
		

	}

}
