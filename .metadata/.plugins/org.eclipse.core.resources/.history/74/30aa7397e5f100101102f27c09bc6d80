package exam;

public class TypeCasting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*
		int num = 10;
		float fnum = num;
		double dnum = num;
		
		System.out.println(num);
		System.out.println(fnum);
		System.out.println(dnum);
		*/
		
		float fnum = 10.9f;
		int num = (int)fnum;
		
		System.out.println(fnum);
		System.out.println(num);
		
		
		

	}

}
