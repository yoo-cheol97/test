package exam;

import java.util.Scanner;

public class Q2439 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		final int startCount = 5;
		
		int i = 1;
		int j = 1;
		
		while ( i < 6 ) {
			j = 1;
			while ( j <= startCount - i) {
				System.out.println(" ");
				j++;
			}
			while ( j <= startCount) {
				System.out.print("*");
				j++;
			}
			System.out.print("");
			i++;
		}

	}

}
