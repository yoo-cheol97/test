package exam;

import java.util.Scanner;

public class Q25314 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner keyboard = new Scanner(System.in);
		
		int n = 0;
		
		n = keyboard.nextInt();
		
		String type ="";
		
		while(n >= 4) {
			
			type = "long int";
			n -= 4;
			
		}
		
		System.out.println(type);
		
		//String type = "long ";
		//type = type + "long ";
		
		
		
		

	}

}
