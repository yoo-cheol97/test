package exam;

import java.util.Scanner;

public class Q8393 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner keyboard = new Scanner(System.in);
		
		int n = 0;
		
		n = keyboard.nextInt();
		
		while(1 <= n) {
			
			System.out.println(1+ n);
			
		}

	}

}
