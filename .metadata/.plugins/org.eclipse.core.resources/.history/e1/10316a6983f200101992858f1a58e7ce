package exam;

public class Q120820 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int age = 40;
		int year = 2022;
		int birthYear = year - age + 1;
		
		System.out.println(birthYear);
		
		age = 23;
		year = 2000;
		birthYear =  year - age +1;
		System.out.println(birthYear);
				

	}

}
