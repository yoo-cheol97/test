	//파라미터로 전달된 나이가 성인인지 미성년인지 구분하여 출력한다.
	//   - printAge(20) => "성인"
	//   - printAge(15) => "미성년"
	
	public static int adult(int age) {
		
		String a = "성인";
		String b = "미성년";
		String result;
		
		if(age >= 20) {
			result = a;
		}
		else {
			result = b;
		}
		return age;
	}
	
	public static void main(String[] args) {
		int printAge = adult(20);
		System.out.println(printAge);

	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
