package exam;

import java.util.Scanner;

public class Q2438 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
				
		Scanner keyboard = new Scanner(System.in);
		
		int j = 0;
		int i = 0;
		
		j = keyboard.nextInt();
		
		int n = 2;
		int o = 1;
		
		while (n < 10) {
			
			i = 1;
			while( o < 10) {
				
				System.out.println(n + "*" + o + "=" + n * o);
				o++;
			}
			
			n++;
		}

	}

}
